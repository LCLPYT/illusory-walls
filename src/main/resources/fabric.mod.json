{
	"schemaVersion": 1,
	"id": "illwalls",
	"version": "${version}",
	"name": "Illusory Walls",
	"description": "A Fabric mod to add illusory walls from the Dark Souls series to Minecraft",
	"authors": [
		"LCLP"
	],
	"contact": {
		"homepage": "https://github.com/LCLPYT/illusory-walls",
		"sources": "https://github.com/LCLPYT/illusory-walls"
	},
	"license": "MIT",
	"icon": "assets/illwalls/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"work.lclpnet.illwalls.IllusoryWallsMod"
		],
		"client": [
			"work.lclpnet.illwalls.IllusoryWallsModClient"
		]
	},
	"mixins": [
		{
			"config": "illwalls.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.17",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {}
}